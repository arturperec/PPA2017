<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="some.weird.ppafirebaselogin2017">

<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

<application
    android:name="some.weird.ppafirebaselogin2017.FirebaseApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="some.weird.ppafirebaselogin2017.MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name="some.weird.ppafirebaselogin2017.LoginActivity" />
    <activity android:name="some.weird.ppafirebaselogin2017.SignUpActivity" />
    <activity
        android:name="some.weird.ppafirebaselogin2017.ProfileActivity"
        android:label="@string/title_activity_profile"
        android:theme="@style/AppTheme.NoActionBar" />
    <activity android:name="some.weird.ppafirebaselogin2017.EditProfileActivity">
    </activity>
</application>

</manifest>